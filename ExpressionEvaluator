package com.tdd.code;

public class ExpressionEvaluator {

	public long addTwoInteger(String expression) {
		int operatorposition = expression.indexOf("+");
		long argumentOne = Long.parseLong(expression.substring(0,operatorposition));
		long argumenttwo = Long.parseLong(expression.substring(operatorposition+1));
		return argumentOne+argumenttwo;
	}

